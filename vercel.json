{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    },

    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },

    {

    "src": "views/*",
    "use": "@vercel/static"
  }
  ],

  
  "routes": [
    
    {
      "src": "/(.*)",
      "dest": "/api/app.js"
    }
  ]
}
